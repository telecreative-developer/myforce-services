{
  "host": "localhost",
  "port": 31001,
  "public": "../public/",
  "paginate": {
    "default": 2000,
    "max": 2500
  },
  "mysql": "mysql://root:@localhost:3306/myforce",
  "authentication": {
    "secret":
      "",
    "strategies": ["jwt", "local"],
    "path": "/authentication",
    "service": "users",
    "jwt": {
      "header": {
        "typ": "access"
      },
      "audience": "https://yourdomain.com",
      "subject": "anonymous",
      "issuer": "feathers",
      "algorithm": "HS256",
      "expiresIn": "1d"
    },
    "local": {
      "entity": "user",
      "usernameField": "email",
      "passwordField": "password"
    }
  }
}
